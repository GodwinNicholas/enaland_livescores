<header>
    <div class="overlay"></div>
    <div class="brand-logo">
        <a href="/" class="logo">Enaland <span class="red-text">Live Score</span>
            <!-- <i class="fas fa-futbol blinker"></i> -->
        </a>
    </div>
</header>


<div class="threeSectionalLayout">
    <!-- center column -->
    <main class="event-details">
        <!-- ads -->
        <iframe scrolling='no' frameBorder='0'
            style='padding:0px; margin:0 10px; border:0px;border-style:none;border-style:none;' width='320' height='50'
            src="https://wlbet9ja.adsrv.eacdn.com/I.ashx?btag=a_150622b_702c_&affid=7595&siteid=150622&adid=702&c="></iframe>
        <iframe scrolling='no' frameBorder='0'
            style='padding:0px; margin:0 10px; border:0px;border-style:none;border-style:none;' width='320' height='50'
            src="https://wlbet9ja.adsrv.eacdn.com/I.ashx?btag=a_150622b_700c_&affid=7595&siteid=150622&adid=700&c="></iframe>
        <div class="nav-btn">
            <a href="/" class="btn btn-primary text-white my-2 mx-2">
                <i class="fas fa-arrow-circle-left"></i>
                Go back
            </a>
        </div>
        <div class="table">
            <div class="league-role">
                <div>
                    <table>
                        <thead>
                            <tr>
                                <th><%= match.country_name %></th>
                                <th><%= match.league_name %></th>
                                <th></th>
                                <th><%= match.event_date %></th>
                            </tr>
                        </thead>
                        <tbody>
                            <td>
                                <span class="timer"></span>
                                <%= match.event_status %>
                            </td>
                            <td><%= match.event_home_team %></td>
                            <td><%= match.event_final_result %></td>
                            <td><%= match.event_away_team %></td>
                        </tbody>

                        <% if(odds != null){ %>
                        <thead>
                            <tr>
                                <th>Betting Odds</th>
                                <th>1</th>
                                <th>X</th>
                                <th>2</th>
                            </tr>
                        </thead>
                        <tbody>
                            <td></td>
                            <td>
                                <a href="https://wlbet9ja.adsrv.eacdn.com/PPC.ashx?btag=a_150622b_702c_&affid=7595&siteid=150622&adid=702&c=&channelid=1&MediaID=1246&m=2|2&g=https%3a%2f%2fregister.bet9ja2.com%2f%3fbtag%3da_150622b_702c_%5baid%5d%26utm_source%3daffiliate_7595%26affid%3d7595%26utm_medium%3d150622%26utm_campaign%3d702%26c%3d%5baid%5d%5baddurlparams%5d"
                                    class="p-2 bg-success text-white"><%= odds.odd_1 %></a>
                            </td>
                            <td>
                                <a href="https://wlbet9ja.adsrv.eacdn.com/PPC.ashx?btag=a_150622b_702c_&affid=7595&siteid=150622&adid=702&c=&channelid=1&MediaID=1246&m=2|2&g=https%3a%2f%2fregister.bet9ja2.com%2f%3fbtag%3da_150622b_702c_%5baid%5d%26utm_source%3daffiliate_7595%26affid%3d7595%26utm_medium%3d150622%26utm_campaign%3d702%26c%3d%5baid%5d%5baddurlparams%5d"
                                    class="p-2 bg-success text-white"><%= odds.odd_x %></a>
                            </td>
                            <td>
                                <a href="https://wlbet9ja.adsrv.eacdn.com/PPC.ashx?btag=a_150622b_702c_&affid=7595&siteid=150622&adid=702&c=&channelid=1&MediaID=1246&m=2|2&g=https%3a%2f%2fregister.bet9ja2.com%2f%3fbtag%3da_150622b_702c_%5baid%5d%26utm_source%3daffiliate_7595%26affid%3d7595%26utm_medium%3d150622%26utm_campaign%3d702%26c%3d%5baid%5d%5baddurlparams%5d"
                                    class="p-2 bg-success text-white"><%= odds.odd_2 %></a>
                            </td>
                        </tbody>
                        <%}%>

                        <thead>
                            <tr>
                                <th>Match Details: Goals</th>
                                <th></th>
                                <th></th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <% match.goalscorers.forEach(e => { %>
                        <% if(e.home_scorer.length > 2){ %>
                        <tr>
                            <td>
                                <span class="timer"></span>
                                <%= e.time %>
                            </td>
                            <td><%= e.home_scorer%> <i class="fas fa-futbol"></i></td>
                            <td><%= e.score %></td>
                        </tr>
                        <% } else { %>
                        <tr>
                            <td>
                                <span class="timer"></span>
                                <%= e.time %>
                            </td>
                            <td><%= e.score %></td>
                            <td><%= e.away_scorer%> <i class="fas fa-futbol"></i></td>
                        </tr>
                        <% } %>
                        <%})%>
                        </tbody>

                        <thead>
                            <tr>
                                <th>Match Details: Cards</th>
                                <th></th>
                                <th></th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <% match.cards.forEach(e => { %>
                            <% if(e.home_fault.length > 2){ %>
                            <tr>
                                <td>
                                    <span class="timer"></span>
                                    <%= e.time %>
                                </td>
                                <td>
                                    <img class="ref-card" src="/img/<%= e.card.split(' ')[0] + '.png' %>" alt=""
                                        srcset="">
                                    <%= e.home_fault %>
                                </td>
                                <td>Home</td>
                            </tr>
                            <% } else{ %>
                            <tr>
                                <td>
                                    <span class="timer"></span>
                                    <%= e.time %>
                                </td>
                                <td>
                                    <img class="ref-card" src="/img/<%= e.card.split(' ')[0] + '.png' %>" alt=""
                                        srcset="">
                                    <%= e.away_fault %>
                                </td>
                                <td>Away</td>
                            </tr> <%}%>
                            <% }) %>
                        </tbody>

                        <thead>
                            <tr>
                                <th>Line ups:</th>
                                <th></th>
                                <th></th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody class="teams">
                            <td>
                                <ul>
                                    <% match.lineups.home_team.starting_lineups.forEach(e => {%>
                                    <li><%= e.player %> - <%= e.player_number %></li>
                                    <br>
                                    <%})%>
                                </ul>
                            </td>
                            <td></td>
                            <td>
                                <ul>
                                    <% match.lineups.away_team.starting_lineups.forEach(e => {%>
                                    <li><%= e.player %> - <%= e.player_number %></li>
                                    <br>
                                    <%})%>
                                </ul>
                            </td>
                        </tbody>
                    </table>

                    <!-- stats -->
                    <div class="text-white">
                        <div class="p-1 mx-0 my-2"
                            style="background: #000; display: flex; align-items: center; text-align: center; flex-direction: column;">
                            <p class="text-white ">Statistics</p>
                        </div>
                        <div>
                            <% if(match.statistics.length > 0){%>
                            <% match.statistics.forEach(e => { %>
                            <div class="row">
                                <p class="text-white col-sm-1"><%= e.home %></p>
                                <div class="col-sm-4">
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar"
                                            style="width:<%= ((parseInt(e.home)/(parseInt(e.home) + parseInt(e.away)))*100) %>%"
                                            aria-valuenow="100" aria-valuemin="0" aria-valuemax="100">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <p><%= e.type %></p>
                                </div>
                                <div class="col-sm-4">
                                    <div class="progress">
                                        <div class="progress-bar" role="progressbar"
                                            style="width: <%= ((parseInt(e.away)/(parseInt(e.home) + parseInt(e.away)))*100) %>%"
                                            aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                                    </div>
                                </div>
                                <p class="text-white col-sm-1"><%= e.away %></p>
                            </div>
                            <%})%>
                            <%}%>
                    </div>
                    
                </div>
            </div>
        </div>
    </main>

    <!-- ads section -->
    <section class="ads-section">
            <!-- ADS -->
        <div>
                <iframe scrolling='no' frameBorder='0' style='padding:0px; margin:0px 0px 6rem 0px; border:0px;border-style:none;border-style:none;' width='320' height='50' src="https://wlbet9ja.adsrv.eacdn.com/I.ashx?btag=a_150622b_701c_&affid=7595&siteid=150622&adid=701&c=" ></iframe>
            <script type='text/javascript'
                src="https://wlbet9ja.adsrv.eacdn.com/S.ashx?btag=a_150622b_700c_&affid=7595&siteid=150622&adid=700&c="></script>
                <iframe scrolling='no' frameBorder='0' style='padding:0px; margin:0px; border:0px;border-style:none;border-style:none;' width='320' height='480' src="https://wlbet9ja.adsrv.eacdn.com/I.ashx?btag=a_150622b_46c_&affid=7595&siteid=150622&adid=46&c=" ></iframe>
            </div>
    </section>

</div>